# The train/test net protocol buffer definition
train_net: "/DATA3/caffe/matlab/SP_classification/feature_selection_test/test.prototxt"
base_lr: 0.00001
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.1
stepsize: 2000
# Display every 100 iterations
display: 1
# The maximum number of iterations
max_iter: 100
# snapshot intermediate results
snapshot: 20000
snapshot_prefix: "/DATA3/caffe/examples/SP_classification/model/unary"
# solver mode: CPU or GPU
solver_mode: GPU
