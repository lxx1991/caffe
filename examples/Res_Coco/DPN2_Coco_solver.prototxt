# The train/test net protocol buffer definition
train_net: "examples/DPN2_Coco_val/DPN2_Coco.prototxt"
base_lr: 0.0002
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.1
stepsize: 40000
# Display every 100 iterations
display: 10
# The maximum number of iterations
max_iter: 60000
iter_size: 10
# snapshot intermediate results
snapshot: 10000
snapshot_prefix: "examples/DPN2_Coco_val/model/unary"
# solver mode: CPU or GPU
solver_mode: GPU
