# The train/test net protocol buffer definition
train_net: "examples/DPN2_Coco_val/DPN2_VOC.prototxt"
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.1
stepsize: 5000
# Display every 100 iterations
display: 10
# The maximum number of iterations
max_iter: 16000
iter_size: 10
# snapshot intermediate results
snapshot: 4000
snapshot_prefix: "examples/DPN2_Coco_val/model/finetune"
# solver mode: CPU or GPU
solver_mode: GPU
