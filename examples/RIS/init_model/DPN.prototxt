    num_output: 21
    kernel_size: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layers {
  name: "upscore"
  type: DECONVOLUTION
  bottom: "fc8-VOC"
  top: "upscore"
  blobs_lr: 0
  convolution_param {
    num_output: 21
    pad: 4
    bias_term: false
    kernel_size: 16
    stride: 8
    weight_filler {
      type: "bilinear"
    }
  }
}

layers {
  name: "softmax"
  type: SOFTMAX
  bottom: "upscore"
  top: "softmax"
}