# The train/test net protocol buffer definition
train_net: "/DATA3/caffe/examples/RIS/RIS.prototxt"
type: "Adam"
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "step"
gamma: 0.5
stepsize: 20000
# Display every 100 iterations
display: 1
# The maximum number of iterations
max_iter: 80000
# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "/DATA3/caffe/examples/RIS/model/object2"
# solver mode: CPU or GPU
solver_mode: GPU
